class Solution {
public:
    int firstUniqChar(string s) {
        int n=s.size();
        int freq[26]={0};
        for(int i=0;i<n;i++){
            int temp=s[i]-'a';
            freq[temp]++;
        }
        for(int i=0;i<n;i++){
            if(freq[s[i]-'a']==1)
                return i;
        }
        return -1;
        
    }
};
