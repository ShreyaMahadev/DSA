// 2 Pointer Approach 
// i-> used in keeping track of the unique/distinct array
// j-> used in scanning the array

class Solution {
public:
    int removeDuplicates(vector<int>& arr) {
        int i=0;    //keeps track of the unique element
        int n=arr.size();
        for(int j=1;j<n;j++)    //start iteration from 2nd element
        {
            if(arr[j]!=arr[i])    //Distinct or unique elements
            {
                arr[i+1]=arr[j]; //put current element next to i(i=unique element)
                i++;    //Increment i to the unique elemment
            }
        }
        return (i+1);   //Length of unique elements
        
    }
};

TC : O(n)
SC : O(1)
