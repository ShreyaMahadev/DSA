//Using unordered_set

class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        unordered_set<int> st;

        for(int i=0;i<nums.size();i++)
        {
            // If current number already exists in the set,
            if(st.find(nums[i])!=st.end())  
                return nums[i]; // it means it's a duplicate, so return it.
            else
                st.insert(nums[i]);// Otherwise, insert the number into set.
        }
        return 0;
        
    }
};

TC : O(N)
SC : O(N)
